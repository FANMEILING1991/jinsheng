{
  "version": 2,
  "builds": [
    {
      "src": "backend/app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/app/main.py"
    }
  ],
  "env": {
    "ENV": "production"
  },
  "functions": {
    "backend/app/main.py": {
      "maxDuration": 30
    }
  }
}
